#!/usr/bin/python
import lxml.html
import requests
page = requests.get('http://quotes.toscrape.com/')
tree = lxml.html.fromstring(page.content)
authors = tree.xpath('//small[@class="text"]/text()')
print ('Authors: ',authors)
